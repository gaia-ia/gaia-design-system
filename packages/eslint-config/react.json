{
  "plugins": [
    "simple-import-sort"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "semi": true
      }
    ],
    "no-useless-constructor": "off"
  },
  "extends": "@rocketseat/eslint-config/react"
}